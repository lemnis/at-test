{
    "name": "caniat",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "playwright test",
        "codeceptjs": "codeceptjs run --steps",
        "codeceptjs:headless": "HEADLESS=true codeceptjs run --steps",
        "codeceptjs:ui": "codecept-ui --app",
        "codeceptjs:run:firefox": "cd output/playwright-firefox && BROWSER=firefox npx codeceptjs run --config ../../codecept.conf.js",
        "codeceptjs:run:webkit": "cd output/playwright-webkit && BROWSER=webkit npx codeceptjs run --config ../../codecept.conf.js",
        "codeceptjs:run:chromium": "cd output/playwright-chromium && BROWSER=chromium npx codeceptjs run --config ../../codecept.conf.js",
        "codeceptjs:all": "npm-run-all -c --parallel codeceptjs:run:*",
        "codeceptjs:report": "npm-run-all -c --parallel \"codeceptjs:run:* -- --reporter ../../codeceptjs/reporter.js\""
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@codeceptjs/configure": "^0.8.0",
        "@codeceptjs/ui": "^0.4.6",
        "@guidepup/playwright": "^0.6.1",
        "@playwright/test": "^1.17.1",
        "@types/chai": "^4.3.1",
        "@types/node": "^17.0.36",
        "chai": "^4.3.6",
        "codeceptjs": "^3.3.3",
        "npm-run-all": "^4.1.5",
        "snap-shot-it": "^7.9.6",
        "ts-node": "^10.8.0",
        "typescript": "^4.7.2"
    },
    "dependencies": {
        "playwright": "^1.23.3"
    }
}
